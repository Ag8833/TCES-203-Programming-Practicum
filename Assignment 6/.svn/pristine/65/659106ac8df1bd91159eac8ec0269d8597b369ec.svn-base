#ifndef STACK_H_
#define STACK_H_

#include "List.h"

class Stack : public List
{
	public:
		Stack();//Default constructor.
		void pushStack(Node*);//Function prototype to push a node to the stack.
		void popStack();//Function prototype to pop a node from the stack.
		void printStack();//Function prototype to print the stack.
		friend ostream &operator<<(ostream &, Stack*);//Function prototype to overload <<.
};

#endif /* STACK_H_ */
